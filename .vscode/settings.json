{
  "Lua.diagnostics.globals": [
    "Events",
    "getGameTime",
    "getCore",
    "getClimateManager",
    "forceSnowCheck",
    "ZombRand",
    "writeLog",
    "getPlayer",
    "ProceduralDistributions"
  ],
  "Lua.workspace.library": ["${addons}/umbrella-unstable/module/library"],
  "Lua.runtime.version": "Lua 5.1",
  "Lua.runtime.path": ["?.lua", "?/init.lua", "server/?.lua"],
  "Lua.completion.requireSeparator": "/",
  "Lua.workspace.preloadFileSize": 800,
  "Lua.runtime.builtin": {
    "debug": "disable",
    "io": "disable",
    "package": "disable"
  },
  "Lua.workspace.checkThirdParty": false,
  "[plaintext]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "pzSyntaxExtension.searchDirectories": [
    "~/project-zomboid-app/Contents/Java/media/scripts"
  ]
}
